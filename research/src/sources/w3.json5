      "name": "Carousel",
      "definition": "Slide Show or Image Rotator",
      "url": "https://www.w3.org/TR/wai-aria-practices/#carousel",
      "anatomy": [
        {
          "name": "slide",
          "description": "A single content container within a set of content containers that hold the content to be presented by the carousel."
        },
        {
          "name": "rotation control",
          "description": "An interactive element that stops and starts automatic slide rotation."
        },
        {
          "name": "slide picker controls",
          "description": "A group of elements, often styled as small dots, that enable the user to pick a specific slide in the rotation sequence to display."
        },
        {
          "name": "previous slide control",
          "description": "An interactive element, often styled as an arrow, that displays the previous slide in the rotation sequence."
        },
        {
          "name": "next slide control",
          "description": "An interactive element, often styled as an arrow, that displays the next slide in the rotation sequence."
        }
      ],
      "concepts": [
